<%= form_with(url: search_path, method: 'get', remote: true, enforce_utf8: false, id: 'search-form') do |f| %>
  <div class='inner-addon left'>
    <%= fa_icon 'search', class: 'search-icon' %>
    <%= f.text_field :q, placeholder: 'Search', class: 'search-form typeahead', id: 'autocomplete' %>
  </div>
<% end %>